{
    "sourceMaps": true,
    "presets": [
        [
            "es2015",
            {
                "loose": true
            }
        ]
    ],
    "plugins": [
        [
            "transform-react-jsx",
            {
                "pragma": "h"
            }
        ],
        "transform-class-properties",
        "transform-es2015-arrow-functions"
    ]
}